void setup(){
  for(int i=2; i<10; i++){
    pinMode (i, OUTPUT);}
  Serial.begin(9600);
}

void loop(){
  if (Serial.available()>0){
      char N= Serial.read();
      int p=(N-'0')+1;
      int x= digitalRead (p);
      if (x == 0){
        digitalWrite (p, HIGH);
      }
      else { 
      digitalWrite (p, LOW);
    }
  }
}
